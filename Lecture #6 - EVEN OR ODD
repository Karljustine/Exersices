#include <iostream>
using namespace std;
int main()
{
    cout << "Enter the number to see whether its even or odd\n";
    int number;
    cin >> number;
    if (number % 2 == 0)
    {
        cout << "\nThe entered number is Even\n";
    }
    else if (number % 2 != 0)
    {
        cout << "The number entered is odd";
    }
    else
    {
        cout << "Incorrect";
    }
}
